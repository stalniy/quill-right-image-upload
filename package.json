{
  "name": "quill-right-image-upload",
  "version": "0.0.1",
  "description": "a plugin for uploading images in Quill. Done right!",
  "main": "./dist/umd.js",
  "module": "./dist/es5m.js",
  "es2016": "./dist/es.js",
  "scripts": {
    "build": "rollup -c tools/rollup.js",
    "lint": "eslint src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
